{
  "title": "PartnerBillRequest",
  "type": "object",
  "description": "Requête permettant d'établir une facture inter-partenaires, pour un triplet (dossier, plan, simulation) donné.",
  "properties": {
    "case_id": {
      "type": "string"
    },
    "plan_id": {
      "type": "string"
    },
    "simulation_id": {
      "description": "Date d'échéance.",
      "format": "date-time",
      "type": "string"
    },
    "partner_from_id": {
      "description": "Montant de la facture.",
      "type": "string"
    },
    "partner_to_id": {
      "type": "string"
    }
  },
  "required": [
    "case_id",
    "plan_id",
    "simulation_id",
    "partner_from_id",
    "partner_to_id"
  ]
}